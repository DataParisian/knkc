 * Purpose: Tests whether a number is prime. 
*/

#include <stdbool.h>
#include <stdio.h>

bool is_prime(int n){
	int divisor;	
	if (n <= 1)
		return false;
	for (divisor = 2; (divisor * divisor) <= n; divisor++){
		if ((n % divisor) == 0)
			return false;	
	}
	return true;
}

int main (void){

	print_pun();
	
	return 0;
}

